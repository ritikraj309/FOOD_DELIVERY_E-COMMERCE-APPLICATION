	<script>
	  	$(document).ready(function(){
	  		event = "event=getHotel";
	  		$.ajax({
	  				url:"hotelServlet",
	  				data:event,
	  				type:"POST",
	  				dataType:"json",
	  				success:function(data,textStatus,jqXHR){ 					 
	  					//console.log(data);
	  					let i=1;
	  					 let s="";
	  					for (var key in data) {
	  						if (data.hasOwnProperty(key)) {
	  							
	  							 s+="<div class='col-md-12 col-xl-10'>";
	  							 s+="<div class='card shadow-0 border rounded-3'>";
	  							 s+="<div class='card-body'>";
	  							 s+="<div class='row'>";
	  							 s+="<div class='col-md-12 col-lg-3 col-xl-3 mb-4 mb-lg-0'>";
	  							 s+="<div class='bg-image hover-zoom ripple rounded ripple-surface'>";
	  							 s+="<img src='admin/assets/hotelImg/"+data[key].hotel_image+"' class='w-50 h-50' alt=''>";
	  							 s+="<a ><div class='hover-overlay'><divclass='mask' style='background-color: rgba(253, 253, 253, 0.15);'></div></div>";				           
		  				         s+="</a></div></div>";
		  				         s+="<div class='row'>";
		  				         s+="<h2 style='color:green'>"+data[key].hotel_name+"</2>"; 
		  				         s+="<p  class='text-truncate mb-4 mb-md-0'>"+data[key].hotel_description+"</p>"; 
		  				         s+="</div>";
		  				         s+="<div class='col-md-6 col-lg-3 col-xl-3 border-sm-start-none border-start'>";
		  				         s+="<a style='color:red' href='details.jsp?hotel_id="+data[key].hotel_id+"'>Order Now";
		  				         s+="</a></div></div></div></div></div>";
	  							 			     						   
	  						}
	  					}
	  					$('#hotels').html(s); 
	  					console.log('success..')
	  				},
					error:function(jqXHR,textStatus,errorThrown){
						console.log(jqXHR.responseText);
	  					console.log("error...")  					
	  				} 			
	  		});
